{
  "name": "@javacs3/xterm-player",
  "version": "0.0.3",
  "description": "A web player for xterm recording",
  "style": "./dist/css/xterm-player.css",
  "main": "./dist/js/xterm-player.js",
  "types": "./typings/xterm-player.d.ts",
  "repository": "https://github.com/JavaCS3/xterm-player",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "author": "javacs3",
  "license": "MIT",
  "scripts": {
    "lint": "tslint 'src/**/*.ts'",
    "clean": "rm -rf out/",
    "test": "jest && yarn lint",
    "watch": "jest --watch --color",
    "demo": "webpack-dev-server --config build/webpack.config.dev.js --open",
    "dev": "yarn run demo",
    "build": "tsc -b ./tsconfig.json && yarn lint",
    "build:docs": "NODE_ENV=production webpack --optimize-minimize --config ./build/webpack.config.docs.js",
    "package": "NODE_ENV=production webpack --optimize-minimize --config ./build/webpack.config.prod.js"
  },
  "dependencies": {
    "xterm": "^4.4.0",
    "xterm-addon-serialize": "^0.1.2"
  },
  "devDependencies": {
    "@types/jest": "^25.1.1",
    "@types/node": "^13.7.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.7.1",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "style-loader": "^0.23.1",
    "svg-inline-loader": "^0.8.2",
    "ts-jest": "^24.0.2",
    "ts-loader": "^5.3.3",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-consistent-codestyle": "^1.16.0",
    "tslint-webpack-plugin": "^2.0.4",
    "typescript": "^3.4.4",
    "typescript-tslint-plugin": "^0.3.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "vue": "^2.6.10",
    "webpack": "^4.30.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1"
  }
}
